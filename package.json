{
  "name": "nfp77",
  "version": "0.0.1",
  "description": "Dashboard for the National Research Program 77 Dashboard",
  "productName": "Swiss Job Tracker",
  "author": "Matt Bannert <matthias.bannert@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "test": "jest",
    "test-update": "jest --updateSnapshot",
    "build": "quasar build",
    "predeploy": "quasar build",
    "deploy": "push-dir --dir=dist/spa --branch=gh-pages --cleanup",
    "dev": "quasar dev",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "lint-staged": {
    "*.{ts,vue,js}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,md}": "prettier --write"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.3",
    "core-js": "^3.6.5",
    "debug": "^4.3.2",
    "plausible-tracker": "^0.3.8",
    "push-dir": "^0.4.1",
    "quasar": "^2.12.0",
    "vue": "^3.3.2",
    "vue-echarts": "^6.2.3",
    "vue-i18n": "^9.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@quasar/app-webpack": "^3.9.2",
    "babel-jest": "^29.5.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.12.0",
    "eslint-webpack-plugin": "^4.0.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
